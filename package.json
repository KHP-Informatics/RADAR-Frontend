{
  "name": "radar-dashboard",
  "version": "2.1.0",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:tools": "ng serve --environment=tools",
    "start:host": "ng serve --host 0.0.0.0",
    "build": "ng build --prod --build-optimizer --progress=false",
    "build:dev": "ng build",
    "all": "yarn fix:all && yarn lint && yarn test:coverage && yarn e2e",
    "lint": "yarn lint:ng && yarn lint:css && yarn lint:prettier && yarn lint:sort",
    "lint:ng": "ng lint --type-check",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:prettier": "prettier --list-different \"**/*(*.js|*.ts|*.json)\"",
    "lint:sort": "import-sort --list-different \"src/app/**/*(*.js|*.ts)\"",
    "test": "ng test --environment=test",
    "test:coverage": "ng test --single-run --code-coverage && open coverage/index.html",
    "e2e": "ng e2e",
    "fix:all": "yarn fix:sort && yarn fix:css && yarn fix:code",
    "fix:css": "yarn fix:stylelint",
    "fix:stylelint": "stylelint --fix \"src/**/*.scss\"",
    "fix:code": "yarn fix:prettier",
    "fix:prettier": "prettier --write \"**/*(*.js|*.ts|*.json)\"",
    "fix:sort": "import-sort --write \"src/app/**/*(*.js|*.ts)\"",
    "deploy": "now --public --docker",
    "deploy:alias": "now alias"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "now": {
    "name": "radar-dashboard-canary",
    "alias": "radar-dashboard-canary"
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.0.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.0.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@auth0/angular-jwt": "^1.0.0",
    "@ngrx/effects": "^5.1.0",
    "@ngrx/entity": "^5.1.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.1.0",
    "core-js": "^2.5.3",
    "d3": "^5.3.0",
    "d3-line-chunked": "^1.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.5.6",
    "shortid": "^2.2.8",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@ngrx/schematics": "^5.2.0",
    "@ngrx/store-devtools": "^5.1.0",
    "@types/d3": "^5.0.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.8.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^9.3.0",
    "browserstack-local": "^1.3.0",
    "codacy-coverage": "^3.0.0",
    "codelyzer": "^4.0.2",
    "coveralls": "^3.0.0",
    "enhanced-resolve": "^4.0.0-beta.4",
    "import-sort-cli": "^4.2.0",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-parser-typescript": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-scss-preprocessor": "^3.0.0",
    "karma-spec-reporter": "^0.0.32",
    "ngrx-store-freeze": "^0.2.0",
    "node-sass": "^4.8.3",
    "now": "^11.1.7",
    "prettier": "^1.10.2",
    "protractor": "^5.2.2",
    "rxjs-tslint-rules": "^4.1.1",
    "sass-mq": "^4.0.2",
    "stylelint": "^9.0.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0",
    "tslint-language-service": "^0.9.8",
    "typescript": "~2.6"
  }
}
