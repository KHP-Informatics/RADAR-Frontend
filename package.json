{
  "name": "radar-dashboard",
  "version": "0.0.1",
  "license": "Apache 2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:host": "ng serve --host 0.0.0.0",
    "hmr": "ng serve --hmr -e=hmr",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "all": "npm run fix:css && npm run lint:all && npm run test:coverage && npm run e2e",
    "lint": "ng lint --type-check",
    "lint:css": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "ng test",
    "test:ci": "ng test --code-coverage --watch=false",
    "test:coverage": "ng test --code-coverage --watch=false && open coverage/index.html",
    "e2e": "ng e2e",
    "fix:css": "postcss \"src/**/*.scss\" --replace"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.3",
    "@angular/common": "^4.2.3",
    "@angular/compiler": "^4.2.3",
    "@angular/core": "^4.2.3",
    "@angular/forms": "^4.2.3",
    "@angular/http": "^4.2.3",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "^4.2.3",
    "@angular/platform-browser-dynamic": "^4.2.3",
    "@angular/router": "^4.2.3",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.3",
    "@ngrx/store": "^2.2.2",
    "core-js": "^2.4.1",
    "d3": "^4.9.1",
    "hammerjs": "^2.0.8",
    "reselect": "^3.0.1",
    "rxjs": "^5.4.1",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.2",
    "@angular/compiler-cli": "^4.2.3",
    "@angular/language-service": "^4.2.3",
    "@ngrx/store-devtools": "^3.2.4",
    "@types/d3": "^4.9.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.52",
    "@types/node": "^8.0.0",
    "browserstack-local": "^1.3.0",
    "codacy-coverage": "^2.0.2",
    "codelyzer": "^3.0.1",
    "coveralls": "^2.13.1",
    "enhanced-resolve": "^3.1.0",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "^0.1.9",
    "postcss": "^6.0.2",
    "postcss-cli": "^4.1.0",
    "postcss-scss": "^1.0.1",
    "postcss-sorting": "^3.0.0",
    "protractor": "^5.1.2",
    "sass-mq": "^3.3.2",
    "stylefmt": "^6.0.0",
    "stylelint": "^7.11.1",
    "stylelint-config-standard": "^16.0.0",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "tslint-config-standard": "^6.0.1",
    "tslint-language-service": "^0.9.6",
    "typescript": "2.3.4"
  }
}
