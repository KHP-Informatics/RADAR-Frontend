{
  "name": "radar-dashboard",
  "version": "2.1.0",
  "license": "Apache 2.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:tools": "ng serve --environment=tools",
    "start:host": "ng serve --host 0.0.0.0",
    "build": "ng build --prod --build-optimizer --stats-json --progress=false",
    "build:dev": "ng build",
    "all": "yarn fix:all && yarn lint && yarn test:coverage && yarn e2e",
    "lint": "yarn lint:ng && yarn lint:css && yarn lint:prettier && yarn lint:sort",
    "lint:ng": "ng lint --type-check",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:prettier": "prettier --list-different \"**/*(*.js|*.ts|*.json)\"",
    "lint:sort": "import-sort -d -s src/app",
    "test": "ng test --environment=test --sourcemaps=false",
    "test:coverage": "ng test --single-run --code-coverage && open coverage/index.html",
    "e2e": "ng e2e",
    "fix:all": "yarn fix:sort && yarn fix:css && yarn fix:code",
    "fix:css": "yarn fix:stylefmt && yarn fix:stylelint",
    "fix:stylefmt": "postcss -r \"src/**/*.scss\"",
    "fix:stylelint": "stylelint --fix \"src/**/*.scss\"",
    "fix:code": "yarn fix:prettier",
    "fix:prettier": "prettier --write \"**/*(*.js|*.ts|*.json)\"",
    "fix:sort": "import-sort -o src/app || true"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "module"
    },
    ".ts": {
      "parser": "typescript",
      "style": "module"
    }
  },
  "browserslist": [
    "last 2 versions",
    "not ie <= 10",
    "not ie_mob <= 10"
  ],
  "dependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "5.0.0-rc0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@ngrx/effects": "^4.1.0",
    "@ngrx/entity": "github:ngrx/entity-builds",
    "@ngrx/store": "^4.1.0",
    "core-js": "^2.5.1",
    "d3": "^4.11.0",
    "d3-line-chunked": "^1.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.5.2",
    "shortid": "^2.2.8",
    "ts-helpers": "^1.1.2",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@ngrx/store-devtools": "^4.0.0",
    "@types/d3": "^4.11.0",
    "@types/hammerjs": "^2.0.35",
    "@types/jasmine": "^2.6.1",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.0.47",
    "browserstack-local": "^1.3.0",
    "codacy-coverage": "^2.0.3",
    "codelyzer": "^4.0.1",
    "coveralls": "^3.0.0",
    "enhanced-resolve": "^3.4.1",
    "import-sort-cli": "^3.3.0",
    "import-sort-parser-babylon": "^3.2.0",
    "import-sort-parser-typescript": "^3.2.0",
    "import-sort-style-module": "^3.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-scss-preprocessor": "^3.0.0",
    "karma-spec-reporter": "0.0.31",
    "ngrx-store-freeze": "^0.2.0",
    "postcss": "^6.0.14",
    "postcss-cli": "^4.1.1",
    "postcss-scss": "^1.0.2",
    "prettier": "^1.7.4",
    "protractor": "^5.2.0",
    "rxjs-tslint-rules": "^3.0.4",
    "sass-mq": "^3.3.2",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-language-service": "^0.9.6",
    "typescript": "^2.6.1"
  }
}
