{
  "name": "radar-dashboard",
  "version": "0.0.1",
  "license": "Apache 2.0",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:host": "ng serve --host 0.0.0.0",
    "hmr": "ng serve --hmr -e=hmr",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "lint": "ng lint",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "test": "ng test",
    "test:ci": "ng test --code-coverage --watch=false",
    "test:coverage": "ng test --code-coverage --watch=false && open coverage/index.html",
    "pree2e": "webdriver-manager update --standalone false --gecko",
    "e2e": "ng e2e --no-webdriver-update",
    "css": "postcss \"src/**/*.scss\" --replace"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/forms": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "d3": "4.7.2",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.1.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-beta.32.3",
    "@angular/compiler-cli": "^2.4.8",
    "@angularclass/hmr": "^1.2.2",
    "@ngrx/store-devtools": "^3.2.2 ",
    "@types/d3": "^4.5.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "browserstack-local": "^1.3.0",
    "codacy-coverage": "^2.0.1",
    "codelyzer": "^2.0.1",
    "coveralls": "^2.11.16",
    "enhanced-resolve": "^3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "karma": "^1.5.0",
    "karma-browserstack-launcher": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.3",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "^0.1.9",
    "postcss": "^5.2.14",
    "postcss-cli": "^4.0.0",
    "postcss-scss": "^0.4.1",
    "postcss-sorting": "^2.0.1",
    "protractor": "~5.1.0",
    "sass-mq": "^3.3.2",
    "stylefmt": "^5.1.2",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "ts-node": "^2.1.0",
    "tslint": "~4.4.2",
    "tslint-config-standard": "^4.0.0",
    "typescript": "^2.1.6"
  }
